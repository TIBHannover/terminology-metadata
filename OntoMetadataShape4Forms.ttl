### add a list to make easier for forms
    sh:property [
        sh:path <https://schema.org/inLanguage>  ;
        sh:severity sh:Info ;
        #sh:minCount 1 ;
        sh:nodeKind sh:IRI ;
        # sh:pattern "(^http://id.loc.gov/vocabulary/iso639-2/[a-z]{3}$|^https://id.loc.gov/vocabulary/iso639-2/[a-z]{3}$)" ; # for form replaced by list
        sh:in (
