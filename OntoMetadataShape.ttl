            [sh:hasValue <http://w3id.org/nkos/nkostype#glossary>]
            [sh:hasValue <http://w3id.org/nkos/nkostype#list>]
            [sh:hasValue <http://w3id.org/nkos/nkostype#name_authority_list>]
            [sh:hasValue <http://w3id.org/nkos/nkostype#ontology>]
            [sh:hasValue <http://w3id.org/nkos/nkostype#pick_list>]
            [sh:hasValue <http://w3id.org/nkos/nkostype#semantic_network>]
            [sh:hasValue <http://w3id.org/nkos/nkostype#subject_heading_scheme>]
